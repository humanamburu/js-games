    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "redux-immutable": "^3.0.8",
    "reselect": "^2.5.1",
    "url-loader": "^0.5.7",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "cross-env": "^2.0.1",
